install.packages("ggpubr")
library(ggpubr)
Dataset <- read.csv("02-10-2020_1930.csv")
Dataset <- read.csv("02-10-2020_1930.csv")
View(Dataset)
plot( x= Dataset$Confirmed , y = Dataset$Deaths)
plot(
  Dataset$Confirmed,
  Dataset$Deaths,
  col = "red",
  pch = 19,
  xlab = "Confirmed Cases",
  ylab = "Deaths",
  main = "COVID-19 Confirmed Cases vs Deaths"
)
cor(Dataset$Confirmed , Dataset$Deaths)

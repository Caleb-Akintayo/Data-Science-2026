install.packages("caret",dependencies = TRUE)
q()
library(caret)
set.seed(123)
data(iris)
train_index <- createDataPartition(iris$Species, p = 0.8, list = FALSE)
train_data  <- iris[train_index, ]
valid_data  <- iris[-train_index, ]
ctrl <- trainControl(method = "cv", number = 5)
svm_model <- train(
  Species ~ .,
  data = train_data,
  method = "svmRadial",
  trControl = ctrl
)
print(svm_model)
svm_preds <- predict(svm_model, newdata = valid_data)
print(confusionMatrix(svm_preds, valid_data$Species))
save.image("C:\\Users\\cakintayo\\Documents\\Final")
q()
#Caleb Akintayo January 21, 2026, Correlation Test
if(!require(devtools)) install.packages("devtools") devtools::install_github("kassambara/ggpubr")
library("ggpubr")
my_data <- mtcars 
head(my_data, 6)
ggscatter(my_data, x = "mpg", y = "wt", add = "reg.line", conf.int = TRUE, cor.coef = TRUE, cor.method = "pearson", xlab = "Miles/(US) gallon", ylab = "Weight (1000 lbs)")
res <- cor.test(my_data$wt, my_data$mpg, method = "pearson") 
res
save.image("C:\\Users\\cakintayo\\Downloads\\Correlation Test")
